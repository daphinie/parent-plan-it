{
  "expo": {
    "name": "parentPlanIt",
    "slug": "parentPlanIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash":
    {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#d1d7e2",
      "showDuration": 5000, 
      "hideDuration": 1000 
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}